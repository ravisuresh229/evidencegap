{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.py"
    },
    {
      "src": "/api/analyze", 
      "dest": "/api/analyze.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
} 